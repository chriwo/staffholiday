{
	"name": "chriwo/staffholiday",
	"type": "typo3-cms-extension",
	"license": "GPL-3.0",
	"description": "TYPO3 extension for the planning of holidays for employees",
	"keywords": [
		"TYPO3",
		"extension",
		"holiday",
		"vacation",
		"staff",
		"plan"
	],
	"homepage": "http://www.chriwo.de/",
	"author": [
		{
			"name": "Christian Wolfram",
			"role": "Project Manager",
			"homepage": "http://www.chriwo.de"
		}
	],
	"require": {
		"typo3/cms": "^8.7 || >=9.1 <9.3"
	},
	"require-dev": {
		"nimut/testing-framework": "^4"
	},
	"autoload": {
		"psr-4": {
			"ChriWo\\Staffholiday\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ChriWo\\Staffholiday\\Tests\\": "Tests/"
		}
	},
	"replace": {
		"staffholiday": "self.version",
		"typo3-ter/staffholiday": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "staffholiday",
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"app-dir": ".Build",
			"web-dir": ".Build/Web"
		}
	}
}
